  "description": "Shopstore backend",
  "main": "index.js",
  "keywords": [
    "node",
    "fullstack",
    "postgresql"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/node-react-test/node.git"
  },
  "author": {
    "name": "Paulo CÃ©sar da Silva",
    "email": "paulocsmg@gmail.com",
