  },
  "bugs": {
    "url": "https://gitlab.com/node-react-test/node/issues"
  },
  "homepage": "https://gitlab.com/node-react-test/node",
  "scripts": {
    "build": "tsc",
    "debug": "tsnd --inspect --respawn src/index.ts",
    "dev": "tsnd --transpile-only src/index.ts",
    "dev:rest": "SERVICE_ROLE=REST tsnd --transpile-only src/index.ts",
    "dev:queue": "SERVICE_ROLE=QUEUE tsnd --transpile-only src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bull": "^3.22.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "prom-client": "^13.1.0",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.15.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.1",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage/typescript",
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "testTimeout": 10000,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  }
}
